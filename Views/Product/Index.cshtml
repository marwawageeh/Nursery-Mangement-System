@model List<Product>

<table class="table table-bordered table-hover">
	@foreach(var item in Model)
	{
         <tr>
	        <td>@item.ID</td>
			<td>@item.Name</td>
			<td>@item.Price</td>
			<td>@item.Description</td>
			<td>
				<img src="/Images/@item.ImageURL" style="width:80px" />
			</td>
         </tr>
	}
		
</table>
